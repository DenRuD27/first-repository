import java.util.Scanner;

public class SumOfDigits {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a, a2, a3, a4, a5;
        a = in.nextInt();
        a2 = a % 100;
        a3 = a2 % 10;
        a4 = a2 / 10;
        a5 = a / 100;
    System.out.println(a3 + a4 + a5);

    }

}
